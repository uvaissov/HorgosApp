{
  "name": "HorgosApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ios": "react-native run-ios --simulator='iPhone X' && npm run lint-watch",
    "android": "react-native run-android && npm run lint-watch",
    "clean android": "cd android && ./gradlew clean",
    "assembleRelease": "cd android && npm run pwd",
    "pwd": "./gradlew assembleRelease",
    "start": "react-native start",
    "dev-win": "c: && cd %ANDROID_HOME%\\tools && emulator -avd Nexus_5X_API_28",
    "lint": "esw src/**",
    "lint-watch": "esw -w --changed src/**"
  },
  "precommit": "lint",
  "prettier": {
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 2,
    "trailingComma": "none",
    "bracketSpacing": true,
    "semi": false,
    "useTabs": false,
    "jsxBracketSameLine": false
  },
  "dependencies": {
    "@dudigital/react-native-zoomable-view": "^1.0.15",
    "@react-native-community/async-storage": "^1.6.2",
