    "@react-native-community/netinfo": "5.1.0",
    "@react-native-community/viewpager": "^3.3.0",
    "axios": "^0.19.0",
    "i18n-js": "^3.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "16.12.0",
    "react-id-generator": "^3.0.0",
    "react-native": "0.61.5",
    "react-native-easy-grid": "^0.2.2",
