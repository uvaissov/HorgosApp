    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.15.1",
    "eslint-plugin-react-native": "^3.7.0",
    "eslint-watch": "^6.0.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "react-test-renderer": "16.12.0"
  },
  "jest": {
    "preset": "react-native"
  }
